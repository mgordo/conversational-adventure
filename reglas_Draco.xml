	</regla>
	<regla>
		<condiciones num_cond="1">
			<condicion>OBJ_IN_OBJ,83,94,TRUE</condicion>
		</condiciones>
		<consecuencias num_cons="1">
			<consecuencia>LOSE_GAME</consecuencia>
		</consecuencias>
